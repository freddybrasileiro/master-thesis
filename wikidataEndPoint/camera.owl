    <!-- http://www.xfront.com/owl/ontologies/camera/#Large-Format -->

    <owl:Class rdf:about="&camera;Large-Format">
        <rdfs:subClassOf rdf:resource="&camera;Camera"/>
        <rdfs:subClassOf>
            <owl:Restriction>
