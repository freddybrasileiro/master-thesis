@prefix mlt: <http://www.nemo.inf.ufes.br/mlt#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.nemo.inf.ufes.br/mlt#> .


#################################################################
#
#    Classes
#
#################################################################


###  http://www.nemo.inf.ufes.br/mlt#TokenIndividual

mlt:TokenIndividual rdf:type owl:Class ;
                    
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty [ owl:inverseOf mlt:isPowertypeOf
                                                         ] ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass mlt:1stOrderClass
                                        ] ;
                    
                    rdfs:comment "An entity whose instances are instances of Token Individual." ;
					
					rdf:type mlt:1stOrderClass .



###  http://www.nemo.inf.ufes.br/mlt#1stOrderClass

mlt:1stOrderClass rdf:type owl:Class ;
               
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty [ owl:inverseOf mlt:isPowertypeOf
                                                    ] ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass mlt:2ndOrderClass
                                   ] ;
               
               rdfs:subClassOf rdfs:Class ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mlt:isPowertypeOf ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass mlt:TokenIndividual
                               ] ;
               
               rdfs:comment "Particular entities, which are not types, are considered individuals. An entity is an instance of “Individual” iff it is not possibly related to another entity through instantiation." ;
			   
			   rdf:type mlt:2ndOrderClass ;
               
               mlt:isPowertypeOf mlt:TokenIndividual .



###  http://www.nemo.inf.ufes.br/mlt#2ndOrderClass

mlt:2ndOrderClass rdf:type owl:Class ;
               
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty [ owl:inverseOf mlt:isPowertypeOf
                                                    ] ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass mlt:3rdOrderClass
                                   ] ;
               
               rdfs:subClassOf rdfs:Class ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mlt:isPowertypeOf ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass mlt:1stOrderClass
                               ] ;
               
               rdfs:comment "An entity t is a second-order type iff all its instances are first-order types." ;
			   
			   rdf:type mlt:3rdOrderClass ;
               
               mlt:isPowertypeOf mlt:1stOrderClass .



###  http://www.nemo.inf.ufes.br/mlt#3rdOrderClass

mlt:3rdOrderClass rdf:type owl:Class ;
               
               rdfs:subClassOf rdfs:Class ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mlt:isPowertypeOf ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass mlt:2ndOrderClass
                               ] ;
							   
			   rdfs:comment "An entity t is a third-order type iff all its instances are second-order types." ;
			   
			   mlt:isPowertypeOf mlt:2ndOrderClass .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.nemo.inf.ufes.br/mlt#characterizes

mlt:characterizes rdf:type owl:ObjectProperty ;
                  
                  rdfs:subPropertyOf mlt:crossLevelProperty ;
                  
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( mlt:1stOrderClass
                                              mlt:2ndOrderClass
                                              mlt:3rdOrderClass
                                            )
                              ] ;
                  
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( mlt:TokenIndividual
                                             mlt:1stOrderClass
                                             mlt:2ndOrderClass
                                           )
                             ] ;
                  
                  rdfs:comment "The principle of application of t1 defines that its instances applies to instances of t2 according a specific classification criteria. Thus, the extension of t1 is composed by the proper specializations of t2 that follows the specified classification criteria." .



###  http://www.nemo.inf.ufes.br/mlt#completelyCharacterizes

mlt:completelyCharacterizes rdf:type owl:ObjectProperty ;
                            
                            rdfs:subPropertyOf mlt:characterizes ;
                            
                            owl:propertyDisjointWith mlt:incompletelyCharacterizes ;
                            
                            rdfs:comment "A variation of characterization in which the classification criteria defined by the principle of application of t1 guarantees that each instance of t2 is instance of at least one instance of t1." .



###  http://www.nemo.inf.ufes.br/mlt#crossLevelProperty

mlt:crossLevelProperty rdf:type owl:ObjectProperty ;
                       
                       rdfs:subPropertyOf mlt:mltProperty .



###  http://www.nemo.inf.ufes.br/mlt#disjointlyCharacterizes

mlt:disjointlyCharacterizes rdf:type owl:ObjectProperty ;
                            
                            rdfs:subPropertyOf mlt:characterizes ;
                            
                            owl:propertyDisjointWith mlt:overlappinglyCharacterizes ;
                            
                            rdfs:comment "A variation of characterization in which the classification criteria defined by the principle of application of t1 guarantees that each instance of t2 is instance of at most one instance of t1." .



###  http://www.nemo.inf.ufes.br/mlt#incompletelyCharacterizes

mlt:incompletelyCharacterizes rdf:type owl:ObjectProperty ;
                              
                              rdfs:subPropertyOf mlt:characterizes .



###  http://www.nemo.inf.ufes.br/mlt#intraLevelProperty

mlt:intraLevelProperty rdf:type owl:ObjectProperty ;
                       
                       rdfs:subPropertyOf mlt:mltProperty .



###  http://www.nemo.inf.ufes.br/mlt#isPowertypeOf

mlt:isPowertypeOf rdf:type owl:ObjectProperty ;
                  
                  rdfs:subPropertyOf mlt:crossLevelProperty ;
                  
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( mlt:1stOrderClass
                                              mlt:2ndOrderClass
                                              mlt:3rdOrderClass
                                            )
                              ] ;
                  
                  rdfs:comment "The principle of application of t1 defines that its instances applies to instances of t2 but does not define a classification criteria. Thus, the extension of t1 is composed by all specializations of t2, including t2 itself." .



###  http://www.nemo.inf.ufes.br/mlt#isSubordinatedTo

mlt:isSubordinatedTo rdf:type owl:ObjectProperty ;
                     
                     rdfs:subPropertyOf mlt:intraLevelProperty ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( mlt:2ndOrderClass
                                                 mlt:3rdOrderClass
                                               )
                                 ] ;
                     
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( mlt:2ndOrderClass
                                                mlt:3rdOrderClass
                                              )
                                ] ;
                     
                     rdfs:comment """The principle of application of each instance of t1 adds some classification criteria to the principle of application of some instance of t2 i.e. every instance of t1 proper specializes some instance of t2.
Domain and Range: Higher-order types of the same order (instances of 2ndOT or 3rdOT).""" .



###  http://www.nemo.inf.ufes.br/mlt#mltProperty

mlt:mltProperty rdf:type owl:ObjectProperty ;
                
                rdfs:isDefinedBy <http://www.nemo.inf.ufes.br/mlt#> .



###  http://www.nemo.inf.ufes.br/mlt#overlappinglyCharacterizes

mlt:overlappinglyCharacterizes rdf:type owl:ObjectProperty ;
                               
                               rdfs:subPropertyOf mlt:characterizes ;
                               
                               rdfs:isDefinedBy <http://www.nemo.inf.ufes.br/mlt#> .



###  http://www.nemo.inf.ufes.br/mlt#partitions

mlt:partitions rdf:type owl:ObjectProperty ;
               
               rdfs:subPropertyOf mlt:characterizes ;
               
               rdfs:comment "Equivalent to the MLT Cross-level structural relation: completelyCharacterizes and disjointlyCharacterizes. A variation of characterization in which the classification criteria defined by the principle of application of t1 guarantees that each instance of t2 is instance of exactly one instance of t1." .





#################################################################
#
#    General axioms
#
#################################################################


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( mlt:TokenIndividual
                mlt:1stOrderClass
                mlt:2ndOrderClass
                mlt:3rdOrderClass
              )
] .